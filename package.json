{
  "name": "typescript-seed",
  "version": "0.1.0",
  "description": "Typescript seed",
  "scripts": {
    "pretest": "rm -rf dist && tsc",
    "test": "mocha dist/test/**/*.spec.js -R spec --bail",
    "test:cov": "rm -rf coverage && istanbul cover --include-all-sources --dir ./dist/coverage node_modules/mocha/bin/_mocha -- dist/test/**/*.spec.js -R spec --bail",
    "test:remap": "remap-istanbul -i dist/coverage/coverage.json -o coverage -t html",
    "dist": "rm -rf dist && tsc",
    "start": "tsc && node bin/www.js"
  },
  "keywords": [
    "TypeScript",
    "Seed"
  ],
  "author": "Aleix Morgadas <aleixmorgadas@openmailbox.org>",
  "license": "Apache License 2.0",
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.40",
    "@types/mongodb": "^2.1.43",
    "chai": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-typescript": "^1.0.23",
    "remap-istanbul": "^0.9.5",
    "ts-node": "^3.0.2",
    "typescript": "^2.2.2"
  },
  "dependencies": {
    "@reactivex/rxjs": "^5.3.0",
    "@types/body-parser": "^1.16.3",
    "@types/express": "^4.0.35",
    "@types/mongoose": "^4.7.11",
    "@types/node": "^7.0.15",
    "@types/winston": "^2.3.1",
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "jsonwebtoken": "^7.4.0",
    "mongoose": "^4.9.6",
    "winston": "^2.3.1"
  }
}
